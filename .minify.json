{
    "js": {
        "type": "swc",
        "swc": {
            "compress": {
                "arguments": true,
                "arrows": true,
                "booleans": true,
                "booleans_as_integers": true,
                "collapse_vars": true,
                "comparisons": true,
                "computed_props": true,
                "conditionals": true,
                "dead_code": true,
                "directives": true,
                "drop_console": true,
                "drop_debugger": true,
                "evaluate": true,
                "expression": true,
                "hoist_funs": false,
                "hoist_props": false,
                "hoist_vars": false,
                "if_return": true,
                "join_vars": true,
                "keep_classnames": false,
                "keep_fargs": false,
                "keep_fnames": false,
                "keep_infinity": false,
                "loops": true,
                "negate_iife": true,
                "properties": true,
                "reduce_funcs": true,
                "reduce_vars": true,
                "side_effects": true,
                "switches": true,
                "typeofs": true,
                "unsafe": false,
                "unsafe_arrows": false,
                "unsafe_comps": false,
                "unsafe_Function": false,
                "unsafe_math": false,
                "unsafe_symbols": false,
                "unsafe_methods": false,
                "unsafe_proto": false,
                "unsafe_regexp": false,
                "unsafe_undefined": false,
                "unused": true,
                "const_to_let": true,
                "pristine_globals": true
            },
            "mangle": {
                "toplevel": true,
                "keep_classnames": false,
                "keep_fnames": false,
                "keep_private_props": false,
                "ie8": false,
                "safari10": false
            },
            "sourceMap": false
        }
    }
}
